    <script>
        // Tab switching functionality
        document.querySelectorAll('.nav-tab').forEach(tab => {
            tab.addEventListener('click', function() {
                // Remove active class from all tabs and panels
                document.querySelectorAll('.nav-tab').forEach(t => t.classList.remove('active'));
                document.querySelectorAll('.tab-panel').forEach(p => p.classList.remove('active'));
                
                // Add active class to clicked tab and corresponding panel
                this.classList.add('active');
                const tabId = this.getAttribute('data-tab');
                document.getElementById(tabId).classList.add('active');
            });
        });
        
        // View switching functionality
        function switchView(viewName) {
            // Remove active class from all toggle buttons
            document.querySelectorAll('.toggle-btn').forEach(btn => btn.classList.remove('active'));
            
            // Add active class to clicked button
            event.target.classList.add('active');
            
            // Hide all view content
            document.querySelectorAll('.view-content').forEach(view => view.classList.remove('active'));
            
            // Show selected view
            document.getElementById(viewName + '-view').classList.add('active');
        }
        
        // Department toggle functionality
        function toggleDepartment(header) {
            const section = header.parentElement;
            section.classList.toggle('expanded');
        }
        
        // Function item selection
        document.querySelectorAll('.function-item').forEach(item => {
            item.addEventListener('click', function() {
                console.log('Selected function:', this.querySelector('.function-name').textContent);
            });
        });
        
        document.querySelectorAll('.workforce-function-item').forEach(item => {
            item.addEventListener('click', function() {
                console.log('Selected workforce function:', this.querySelector('.workforce-function-name').textContent);
            });
        });
    </script>
</body>
